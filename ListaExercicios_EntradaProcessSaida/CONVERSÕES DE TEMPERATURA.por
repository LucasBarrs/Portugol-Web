programa {
  funcao inicio() {
    
    //variaveis 
    inteiro celsius 
    inteiro fahrenheit

    //entradas 
    escreva ("digite quantos graus Celsius está no momento: ")
    leia (celsius)

    //processamentos 
    fahrenheit = (celsius * 1.8 + 32)

    //saidas
    escreva ("Em Fahrenheit, está: ")
    escreva (fahrenheit)
    escreva (" graus")

  }
}
