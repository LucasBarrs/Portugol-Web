programa {
  funcao inicio() {
    //entradas 
    real anonasci
    real anoatual
    inteiro diasvividos

   //entradas
   escreva ("Digite o ano em que você nasceu: ")
   leia (anonasci)
   escreva ("Digite o ano atual: ")
   leia (anoatual)

   //processamentos
   diasvividos = (anoatual - anonasci) * 365

   //saidas
   escreva ("Esta é a quantidade de dias vividos: ")
   escreva (diasvividos)

    
  }
}
