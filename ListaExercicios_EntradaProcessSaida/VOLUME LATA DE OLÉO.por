programa {
  funcao inicio() {
    //variaveis
    real raio
    real altura
    real volume

    //entradas
    escreva ("digite o valor do raio: ")
    leia (raio)
    escreva ("digite a altura: ")
    leia (altura)

    //processos
    volume = (3.14149 * raio * raio *altura)

    //saida
    escreva ("Esse é o volume da lata de oleo: ")
    escreva (volume)
  }
}
